<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <script src="js/vue.js"></script>
</head>
<body>
    <div id="app">
        <input type="text" v-model="msg">
        {{msg}}
        <div v-once>
            {{msg}}
        </div>
        <div v-cloak>
            {{msg}}
        </div>
    </div>
</body>
<script>
    alert(123);
    new Vue({
        "el" : "#app",
        "data" : {
            "msg" : "vue内置指令练习"
        }
    })
</script>
</html>